-- Load Vape UI Library
local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt"))()

-- Create Main Window
local win = lib:Window("Anti-Jam", Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)

-- Create a New Tab
local tab = win:Tab("Anti-Jam")

-- Initialize Services
local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

-- Initialize Variables
local antiJamEnabled = false
local player = Players.LocalPlayer or Players.PlayerAdded:Wait()
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")

-- Function to Apply Anti-Jam
local function applyAntiJam()
    if not antiJamEnabled then return end

    -- Iterate over all characters in the workspace
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player then
            local otherCharacter = otherPlayer.Character
            if otherCharacter and otherCharacter:FindFirstChild("HumanoidRootPart") then
                local otherHumanoidRootPart = otherCharacter:FindFirstChild("HumanoidRootPart")
                
                -- Check if the player's character is close to another player's character
                local distance = (humanoidRootPart.Position - otherHumanoidRootPart.Position).magnitude
                if distance < 5 then  -- Adjust this distance as needed
                    -- Apply a small part of the player's character to phase through the other player
                    humanoidRootPart.CanCollide = false
                    task.wait(0.1)
                    humanoidRootPart.CanCollide = true
                end
            end
        end
    end
end

-- Add UI Elements
tab:Toggle("Enable Anti-Jam", false, function(state)
    antiJamEnabled = state
end)

-- Continuously apply anti-jam while enabled
RunService.Heartbeat:Connect(function()
    applyAntiJam()
end)

